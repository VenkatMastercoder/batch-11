datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  product_id         String   @id
  title              String
  description        String
  category           String
  price              String
  discountPercentage String
  rating             String
  stock              String
  images             String[]
  thumbnail          String
}
